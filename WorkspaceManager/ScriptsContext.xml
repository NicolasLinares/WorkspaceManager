<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

  <object id="BuildScript" type="WorkspaceManagerTool.Models.Scripts.BuildScript" singleton="true">
    <property name="FILENAME" value="${script.build.filename}" />
    <property name="DEBUG_PARAM" value="${script.build.param.debug}" />
    <property name="RELEASE_PARAM" value="${script.build.param.release}" />
    <property name="BUILD_NUMBER_PARAM" value="${script.build.param.buildnumber}" />
    <property name="ONLYDEV_PARAM" value="${script.build.param.onlydev}" />
    <property name="NOTJS_PARAM" value="${script.build.param.notjs}" />
    <property name="NOTDOC_PARAM" value="${script.build.param.notdoc}" />
  </object>

</objects>